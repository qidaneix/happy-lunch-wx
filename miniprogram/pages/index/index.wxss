/**index.wxss**/
.container {
  height: 100vh;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.btn {
  flex: 0 0 auto;
  margin: 20rpx 0;
}
.list {
  width: 100vw;
  flex: 1 1 auto;
  box-sizing: border-box;
  border: 1rpx solid #eee;
  overflow: auto;
}
.list .item {
  border-bottom: 1rpx solid #eee;
}
.list .name {
  display: flex;
  justify-content: space-between;
}
